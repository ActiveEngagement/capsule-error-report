    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@vue-interface/alert": "^1.0.26",
    "@vue-interface/animate-css": "^0.9.9",
    "@vue-interface/btn": "^0.9.11",
    "@vue-interface/btn-activity": "^0.9.12",
    "@vue-interface/card": "^0.9.0",
    "@vue-interface/date-filter": "^0.8.2",
    "@vue-interface/input-field": "^0.11.2",
    "@vue-interface/slide-deck": "^0.9.8",
    "axios": "^0.22.0",
    "capsule-common": "^1.0.0-beta.1",
    "capsule-editor": "^1.0.0-beta.0",
    "capsule-lint": "^0.1.0",
    "crypto-js": "^4.0.0",
    "jszip": "^3.2.2",
    "vue-hourglass": "^0.1.4",
    "vue-notepad": "^0.2.15",
    "vue-router": "^3.1.6",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-vue": "^7.4.1",
    "sass-loader": "^10.0.4",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
