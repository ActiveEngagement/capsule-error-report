{
  "name": "capsule-error-report",
  "version": "0.1.20",
  "scripts": {
    "commit": "git add -A && git commit -m 'Pre-release commit';",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name CapsuleErrorReport ./src/ErrorReport.vue",
    "build-dev-lib": "NODE_ENV=development vue-cli-service build --target lib --name CapsuleErrorReport ./src/ErrorReport.vue",
    "release-patch": "npm run build && npm run commit && npm version patch; npm run release;",
    "release-minor": "git add -A && npm run commit && npm run build && npm version minor; npm run release;",
    "release-major": "git add -A && npm run commit && npm run build && npm version major; npm run release;",
    "release": "npm run commit; npm publish; git push --follow-tags;"
  },
  "dependencies": {
    "@bugsnag/js": "^6.2.0",
    "@bugsnag/plugin-vue": "^6.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "axios": "^0.18.0",
    "capsule-editor": "^0.3.10",
    "crypto-js": "^3.1.9-1",
    "moment": "^2.24.0",
    "vue-interface": "^0.4.2",
    "vue-notepad": "^0.2.9",
    "vue-router": "^3.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "html-beautify-webpack-plugin": "^1.0.5",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
