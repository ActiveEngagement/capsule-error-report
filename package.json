{
  "name": "capsule-error-report",
  "version": "0.1.4",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name CapsuleErrorReport ./src/Components/ErrorReport.vue",
    "build-dev-lib": "NODE_ENV=development vue-cli-service build --target lib --name CapsuleErrorReport ./src/Components/ErrorReport.vue",
    "lint": "vue-cli-service lint",
    "fix": "./node_modules/.bin/eslint src --ext .js --ext .vue --fix",
    "release-patch": "npm version patch --no-git-tag-version; npm run release;",
    "release-minor": "npm version minor --no-git-tag-version; npm run release;",
    "release-major": "npm version major --no-git-tag-version; npm run release;",
    "release": "npm run fix; npm run build; npm run commit; npm publish; git push;"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/vue-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "crypto-js": "^3.1.9-1",
    "vue": "^2.5.17",
    "vue-code-editor": "file:../vue-code-editor",
    "vue-interface": "^0.2.95"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.2",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-service": "^3.0.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "html-beautify-webpack-plugin": "^1.0.5",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
